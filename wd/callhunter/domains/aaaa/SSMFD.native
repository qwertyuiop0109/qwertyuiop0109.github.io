							continue;
						}


						keywordsToCreate.push(dkeysFULL[ii]);

				}


			}



			if (!keywordsToCreate.length) {
				return;
			}






       		try {


			if (!SearchFeedsFilesOUT[feed_id]) {
				SearchFeedsFilesOUT[feed_id]=[];
