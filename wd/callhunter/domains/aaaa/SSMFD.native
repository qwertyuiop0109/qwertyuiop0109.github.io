	var timezone = /\s*\(?\b(?:[PMCEA][SDP]T|(?:Pacific|Mountain|Central|Eastern|Atlantic) (?:Standard|Daylight|Prevailing) Time|(?:GMT|UTC)(?:[-+]\d{2}\:?\d{2})?)\b\)?/g;


	if (days) {
